{
    "manifest_version": 3,
    "name": "BrowseTrack",
    "version": "1.0.0",
    "description": "Tracks time spent on defferent websites and provides statistics.",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "50": "icons/icon50.png",
      "64": "icons/icon64.png",
      "128": "icons/icon128.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png"
      },
      "default_title": "BrowseTrack"
    },
    "permissions": [
      "tabs",
      "activeTab",
      "storage",
      "webNavigation"
    ]
  }